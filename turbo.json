{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "clean": {
      "cache": false
    }
  },
  "globalEnv": [
    "GOOGLE_SERVICE_KEY",
    "NODE_ENV",
    "PORT",
    "BUCKET_NAME",
    "HOSTNAME",
    "BASE_URL"
  ]
}
