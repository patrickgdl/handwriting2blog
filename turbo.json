{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": [
    "$GOOGLE_SERVICE_KEY",
    "$NODE_ENV",
    "$PORT",
    "$BUCKET_NAME",
    "$HOSTNAME",
    "$PORT",
    "$BASE_URL"
  ],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "clean": {
      "cache": false
    }
  }
}
